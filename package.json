{
  "name": "@nestjsplus/nestjs-package-starter",
  "version": "1.0.0",
  "description": "NestJS npm package starter",
  "author": "John Biundo <johnfbiundo@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "main": "dist/test.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "prebuild": "npm run lint:fix && npm run prettier",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "vitest"
  },
  "keywords": [
    "nestjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nestjsplus/nestjs-package-starter"
  },
  "bugs": "https://github.com/nestjsplus/nestjs-package-starter",
  "peerDependencies": {
    "@nestjs/common": "^10.3.10"
  },
  "dependencies": {},
  "devDependencies": {
    "@nestjs/cli": "^10.3.10",
    "@nestjs/core": "^10.3.10",
    "@eslint/js": "^9.6.0",
    "@nestjs/testing": "^10.3.10",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/eslint__js": "^8.42.3",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "3.3.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.0",
    "vitest": "^2.0.2"
  }
}
